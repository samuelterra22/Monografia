@article{RIVERA,
  abstract = {No mundo globalizado em que vivemos os sistemas, as aplica{\c{c}}{\~{o}}es e as pessoas precisam estar conectadas permanentemente {\`{a}} Internet, e a uma variedade de redes de comunica{\c{c}}{\~{a}}o, com v{\'{a}}rios dispositivos, simultaneamente. O ideal, devido a esse contexto, seria ter um {\'{u}}nico dispositivo com uma {\'{u}}nica interface de rede e um {\'{u}}nico programa que permitisse fazer diversas conex{\~{o}}es com diversos protocolos usados simultaneamente, tornando o sistema simples de usar e f{\'{a}}cil de instalar e operar, conseguindo, desta forma, o n{\'{i}}vel de estabilidade e confiabilidade desejadas. Por isso, as redes de {\'{a}}rea pessoal (WPAN) est{\~{a}}o crescendo em import{\^{a}}ncia desde seu aparecimento em 1991. Diversas tecnologias foram propostas para sua implementa{\c{c}}{\~{a}}o sem que haja um consenso sobre a aplicabilidade de cada op{\c{c}}{\~{a}}o. Este trabalho realiza uma avalia{\c{c}}{\~{a}}o cr{\'{i}}tica das diversas tecnologias propostas e por meio de um laborat{\'{o}}rio de prototipa{\c{c}}{\~{a}}o procurou obter dados que permitam uma avalia{\c{c}}{\~{a}}o mais objetiva dessas tecnologias. Esse documento apresenta uma descri{\c{c}}{\~{a}}o desse laborat{\'{o}}rio e apresenta a metodologia proposta para compara{\c{c}}{\~{a}}o das tecnologias para WPAN., e os resultados obtidos em laborat{\'{o}}rio. O estudo apresentado, demostra, atrav{\'{e}}s de uma avalia{\c{c}}{\~{a}}o critica, que as principais alternativas no momento s{\~{a}}o Wi-Fi e Bluetooth e que com os dados obtidos na prototipa{\c{c}}{\~{a}}o em laborat{\'{o}}rio se provou que hoje h{\'{a}} possibilidade de virtualizar dierentesredes e tipos de interface em uma {\'{u}}nica interface f{\'{i}}sica. A economia de escala, multiplicada pelo efeito da virtualiza{\c{c}}{\~{a}}o das interfaces, leva a conclus{\~{a}}o que as tecnologias Wi-Fi e Bluetooth dever{\~{a}}o dominar o mercado de WLAN e WPAN nos pr{\'{o}}ximos anos. Palavras},
  author = {Rivera, Antonio D'Acosta},
  journal = {Teses.Usp.Br},
  title = {{Redes de equipamentos sem fio de uso pessoal: compara{\c{c}}{\~{a}}o de tecnologias emergentes e an{\'{a}}lise de tend{\^{e}}ncias}},
  url = {http://www.teses.usp.br/teses/disponiveis/3/3142/tde-10012011-084232/en.php},
  year = {2010}
}

